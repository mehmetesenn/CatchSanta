        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="100dp"
            android:layout_height="120dp"
            app:layout_column="0"
            app:layout_row="1"
            android:onClick="skor"
            app:srcCompat="@drawable/nnoel" />
        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="100dp"
            android:layout_height="120dp"
            app:layout_column="1"
            app:layout_row="1"
            android:onClick="skor"
            app:srcCompat="@drawable/nnoel" />
        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="100dp"
            android:layout_height="120dp"
            app:layout_column="2"
            app:layout_row="1"
            android:onClick="skor"
            app:srcCompat="@drawable/nnoel" />
        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="100dp"
            android:layout_height="120dp"
            app:layout_column="0"
            app:layout_row="2"
            android:onClick="skor"
            app:srcCompat="@drawable/nnoel" />
        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="100dp"
            android:layout_height="120dp"
            app:layout_column="1"
            app:layout_row="2"
            android:onClick="skor"
            app:srcCompat="@drawable/nnoel" />
        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="100dp"
